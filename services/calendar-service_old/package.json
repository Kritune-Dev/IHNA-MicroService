{
  "name": "messenger-service",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --force-exit --detectOpenHandles",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test:dev": "jest --detectOpenHandles --watchAll",
    "lint": "standard --verbose | snazzy",
    "node-debug": "DEBUG=express* node src/index.js",
    "chrome-debug": "node --debug-brk --inspect src/index.js",
    "coverage": "jest --coverage",
    "metric": "nodejs-dashboard -- node -r nodejs-dashboard src/index.js",
    "precommit": "npm run lint && npm run test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "nodejs-dashboard": "^0.5.1",
    "nodemon": "^2.0.7",
    "should": "^11.1.2",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^6.1.6",
    "googleapis": "^67.0.0",
    "helmet": "^4.4.1",
    "http-status": "^1.5.0",
    "morgan": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precommit"
    }
  }
}
